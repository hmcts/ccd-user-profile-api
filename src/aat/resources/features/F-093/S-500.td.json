    "invokingUser": {
      "username": "befta.caseworker.1@gmail.com",
      "password": "[[$CCD_BEFTA_CASEWORKER_1_PWD]]"
    }
  },
	"specs": [
		"an active profile in CCD",
    "does not provide valid authentication credentials in CCD",
    "contains HTTP 403 Unauthorized return code"
